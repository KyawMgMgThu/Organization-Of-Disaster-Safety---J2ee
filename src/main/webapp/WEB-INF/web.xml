    <servlet-name>NewsDeleteServlet</servlet-name>
    <servlet-class>auth.NewsDeleteServlet</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>NewsDeleteServlet</servlet-name>
    <url-pattern>/NewsDeleteServlet</url-pattern>
</servlet-mapping>

<servlet>
    <servlet-name>NewsUpdateServlet</servlet-name>
